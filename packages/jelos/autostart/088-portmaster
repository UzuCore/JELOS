. /etc/profile

tocon "Setting portmaster..."
sleep 10

if [ ! -d "/storage/roms/ports/portmaster" ]
then
  ln -s /usr/share/portmaster /storage/roms/ports/portmaster  >/dev/null 2>&1
fi
